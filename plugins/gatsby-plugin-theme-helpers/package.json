{
  "name": "gatsby-plugin-theme-helpers",
  "description": "A plugin to help with Gatsby development.",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "**/test/*.{js,ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme"
  ],
  "bugs": {
    "url": "https://github.com/arshad/gatsby-plugin-theme-helpers/issues"
  },
  "homepage": "https://github.com/arshad/gatsby-plugin-theme-helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com/arshad/gatsby-plugin-theme-helpers.git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lerna": "^3.20.2",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1"
  },
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4"
  }
}
